#include <stdio.h>

int main() {
    int a[3][3];
    int i, j;

    // Taking input
    printf("Enter 9 elements of the matrix:\n");
    for(i = 0; i < 3; i++) {
        for(j = 0; j < 3; j++) {
            scanf("%d", &a[i][j]);
        }
    }

    // Printing transpose
    printf("\nTranspose of the matrix:\n");
    for(i = 0; i < 3; i++) {
        for(j = 0; j < 3; j++) {
            printf("%d ", a[j][i]);
        }
        printf("\n");
    }

    return 0;
}
